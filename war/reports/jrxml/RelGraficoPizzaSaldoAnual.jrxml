	<queryString>
		<![CDATA[select
"public".en_empresa.nm_empresa,
"public".en_parametro.nm_empresa_instalacao,
"public".en_parametro.fones_empresa_instalacao,
"public".en_parametro.de_email_empresa_instalacao,
"public".en_parametro.nu_cnpj_empresa_instalacao,
"public".en_parametro.de_endereco_empresa_instalacao,
consulta.nm_tipo_lancamento,
case when
consulta.saldo_anual < 0 then consulta.saldo_anual * -1
else consulta.saldo_anual end as saldo_anual

from (
SELECT
"public".en_tipo_lancamento.cd_empresa,
"public".en_tipo_lancamento.nm_tipo_lancamento,
(SUM(vl_jan_credito) + SUM(vl_fev_credito) + SUM(vl_mar_credito) +
SUM(vl_abr_credito) + SUM(vl_mai_credito) + SUM(vl_jun_credito) +
SUM(vl_jul_credito) + SUM(vl_ago_credito) + SUM(vl_set_credito) +
SUM(vl_out_credito) + SUM(vl_nov_credito) + SUM(vl_dez_credito)) -
(SUM(vl_jan_debito) + SUM(vl_fev_debito) + SUM(vl_mar_debito) +
SUM(vl_abr_debito) + SUM(vl_mai_debito) + SUM(vl_jun_debito) +
SUM(vl_jul_debito) + SUM(vl_ago_debito) + SUM(vl_set_debito) +
SUM(vl_out_debito) + SUM(vl_nov_debito) + SUM(vl_dez_debito)) as saldo_anual
FROM
"public".en_historico_anual
INNER JOIN "public".en_tipo_lancamento ON "public".en_tipo_lancamento.cd_tipo_lancamento = "public".en_historico_anual.cd_tipo_lancamento
WHERE
"public".en_historico_anual.cd_empresa = 2
and  "public".en_historico_anual.nu_ano = 2018
group by "public".en_tipo_lancamento.nm_tipo_lancamento,"public".en_tipo_lancamento.cd_empresa
) consulta
inner join "public".en_empresa on "public".en_empresa.cd_empresa =  consulta.cd_empresa
cross join "public".en_parametro
order by consulta.nm_tipo_lancamento]]>
	</queryString>
